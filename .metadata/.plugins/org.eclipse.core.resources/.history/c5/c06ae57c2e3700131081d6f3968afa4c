on stdcore[3]: out port motor=XS1_PORT_8A;


#define time_min 20000
#define time_max


void step(unsigned time)
{
	timer tmr;
	unsigned time;
	tmr :> time;



		motor <: 1;
		time+=time_up;
		tmr when timerafter(time) :> void;

